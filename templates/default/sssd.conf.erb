[domain/default]
autofs_provider = ldap
sudo_provider = ldap
krb5_realm = #
ldap_search_base = o=ccs,c=com
id_provider = ldap
auth_provider = ldap
chpass_provider = ldap
ldap_uri = ldap://ldap1
ldap_default_bind_dn = cn=ccs,cn=Clients,o=ccs,c=com
ldap_default_authtok_type = password
ldap_default_authtok = O2Source
ldap_id_use_start_tls = False
cache_credentials = True
#ldap_tls_cacertdir = /etc/openldap/cacerts
#ldap_tls_cacertdir = /usr/ldap/etc
#ldap_tls_cacert = /usr/ldap/etc/clientkey.crl
